{{!
    @template enrol_token/export

    This template renders the exportable html page for the enrol token management page
    It is expected to then be imported to a spreadsheet

    Example context (json):
    {}
}}
<table>
    <thead>
        <tr>
            {{#columns}}<th>{{.}}</th>{{/columns}}
        </tr>
    </thead>
    <tbody>
        {{#rows}}
            <tr>
                {{#.}}<td>{{.}}</td>{{/.}}
            </tr>
        {{/rows}}
    </tbody>
    <tfoot>
        <tr>
            {{#totals}}<th>{{.}}</th>{{/totals}}
        </tr>
    </tfoot>
</table>